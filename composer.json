{
    "name": "bcvsocial/switch-user-stateless-bundle",
    "type": "symfony-bundle",
    "description": "Bundle to use impersonating on Symfony API",
    "keywords": [
        "REST",
        "API",
        "Impersonating"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Chalamon",
            "email": "vincentchalamon@gmail.com",
            "homepage": "http://blog.vincent-chalamon.fr"
        }
    ],
    "require": {
        "php": ">=5.5",
        "symfony/http-kernel": "3.2.*",
        "symfony/dependency-injection": "3.2.*",
        "symfony/http-foundation": "3.2.*",
        "symfony/security-core": "3.2.*",
        "symfony/config": "3.2.*",
        "symfony/security-bundle": "3.2.*",
        "symfony/security-http": "3.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3",
        "doctrine/orm": "2.6.x-dev",
        "doctrine/doctrine-bundle": "~1.4",
        "behat/behat": "^3.0",
        "behat/symfony2-extension": "^2.0",
        "symfony/serializer": "^2.3|^3.0",
        "symfony/browser-kit": "^2.3|^3.0"
    },
    "suggest": {
        "psr/log": "To log user requests",
        "symfony/event-dispatcher": "To dispatch a switch user event"
    },
    "autoload": {
        "psr-4": {
            "BCVSocial\\SwitchUserStatelessBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "BCVSocial\\SwitchUserStatelessBundle\\Tests\\": "features/app"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
